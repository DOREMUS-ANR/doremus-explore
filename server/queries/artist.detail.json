{
  "@context": "http://schema.org",
  "@graph": [{
    "@type": "Person",
    "@id": "?id",
    "name": "$rdfs:label$required$bestlang",
    "additionalName": "$foaf:name$required",
    "birthDate": "$schema:birthDate",
    "deathDate": "$schema:deathDate",
    "birthPlace": "$dbpprop:birthPlace/rdfs:label",
    "deathPlace": "$dbpprop:deathPlace/rdfs:label",
