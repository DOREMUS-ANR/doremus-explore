{
  "name": "overture",
  "version": "0.5.0",
  "description": "Exploring music data in DOREMUS",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"nodemon index\" \"ng serve --aot=false --progress=false --proxy-config proxy.conf.json\"",
    "prebuild": "./node_modules/.bin/stylus ./src/styles/loading.styl -o ./src/styles/loading.css -u rupture -u nib",
    "build": "npm run prebuild && ng build --prod --output-hashing=none && ng build --prod --app overture --output-hashing=none",
    "prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index"
  },
  "dependencies": {
    "@angular/platform-server": "^4.4.2",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "async": "^2.5.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.1",
    "core-js": "^2.5.1",
    "es6-promise": "^4.1.1",
    "es6-shim": "^0.35.3",
    "express": "^4.15.4",
    "express-content-length-validator": "1.0.0",
    "get-json22": "^1.0.0",
    "helmet": "^3.8.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-cache": "^4.1.1",
    "node-monkey": "^1.0.1",
    "node-noop": "^1.0.0",
    "python-shell": "^0.4.0",
    "reflect-metadata": "0.1.10",
    "sparql-client2": "^0.2.0",
    "tsv": "^0.2.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.2",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/cli": "^1.4.2",
    "@angular/common": "^4.4.2",
    "@angular/compiler": "^4.4.2",
    "@angular/compiler-cli": "^4.4.2",
    "@angular/core": "^4.4.2",
    "@angular/forms": "^4.4.2",
    "@angular/http": "^4.4.2",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.4.2",
    "@angular/platform-browser-dynamic": "^4.4.2",
    "@angular/router": "^4.4.2",
    "@types/core-js": "^0.9.43",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.28",
    "angular2-moment": "^1.7.0",
    "bestlang": "0.0.1",
