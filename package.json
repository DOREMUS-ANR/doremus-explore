    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "material-shadows": "^3.0.1",
    "mdi": "^2.0.46",
    "ng-select": "^1.0.0-beta.6",
    "ngx-infinite-scroll": "^0.5.1",
    "nib": "^1.1.2",
    "require-dir": "^0.3.2",
    "rupture": "^0.6.2",
    "rxjs": "^5.4.3",
    "stylus": "^0.54.5",
    "typescript": "^2.3.4",
    "zone.js": "^0.8.17"
  },
  "repository": {
    "type": "git",
