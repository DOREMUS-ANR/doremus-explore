    "stylus": "^0.54.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DOREMUS-ANR/overture.git"
  },
  "author": [
    "Eva Fernandez <eva.fernandez-marin@eurecom.fr>",
