{
  "name": "overture",
  "version": "0.6.1",
  "description": "Exploring music data in DOREMUS",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"nodemon index\" \"ng serve --aot=false --progress=false --proxy-config proxy.conf.json\"",
    "prebuild": "./node_modules/.bin/stylus ./src/styles/loading.styl -o ./src/styles/loading.css -u rupture -u nib",
    "build": "npm run prebuild && ng build --prod --output-hashing=none && ng build --prod --app overture --output-hashing=none",
    "prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index"
  },
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/platform-server": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "async": "^2.6.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "bestlang": "0.0.1",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.3",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "express": "^4.16.2",
    "express-content-length-validator": "1.0.0",
    "fs-extra": "^5.0.0",
    "get-json22": "^1.0.0",
    "helmet": "^3.11.0",
    "jsonfile": "^4.0.0",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "ngx-infinite-scroll": "^0.8.2",
    "node-cache": "^4.1.1",
    "node-monkey": "^1.0.1",
    "node-noop": "^1.0.0",
    "reflect-metadata": "0.1.12",
    "require-dir": "^1.0.0",
    "rxjs": "^5.5.6",
    "sparql-client2": "^0.2.0",
    "sparql-transformer": "^1.0.0",
    "tsv": "^0.2.0",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.0",
    "@angular/compiler-cli": "^5.2.5",
    "@types/core-js": "^0.9.46",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.4.6",
    "concurrently": "^3.5.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.5",
    "material-shadows": "^3.0.1",
    "mdi": "^2.1.19",
    "nib": "^1.1.2",
    "rupture": "^0.7.1",
    "stylus": "^0.54.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DOREMUS-ANR/overture.git"
