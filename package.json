{
  "name": "overture",
  "version": "0.9.0",
  "description": "Exploring music data in DOREMUS",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --raw --kill-others \"nodemon index\" \"ng serve --progress=false --proxy-config proxy.conf.json\"",
    "prebuild": "./node_modules/.bin/stylus ./src/styles/loading.styl -o ./src/styles/loading.css -u rupture -u nib",
    "build": "npm run prebuild && ng build --prod --output-hashing=none && ng run overture:server && npm run postbuild",
    "postbuild": "cpx \"src/styles/loading.css\" dist/styles/",
    "prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/platform-server": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@mdi/font": "^3.3.92",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "accept-language-parser": "^1.5.0",
    "async": "^2.6.1",
    "bestlang": "0.0.1",
    "body-parser": "^1.18.3",
    "clone": "^2.1.2",
    "express": "^4.16.4",
    "express-content-length-validator": "^1.0.0",
    "fs-extra": "^7.0.1",
    "get-json": "^1.0.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.15.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ngx-infinite-scroll": "^7.0.1",
    "node-cache": "^4.2.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "similarity": "^1.1.1",
    "sparql-client2": "^0.2.0",
    "sparql-transformer": "^1.6.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.1.3",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "material-shadows": "^3.0.1",
    "nib": "^1.1.2",
    "rupture": "^0.7.1",
    "stylus": "^0.54.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DOREMUS-ANR/overture.git"
  },
  "author": [
    "Eva Fernandez <eva.fernandez-marin@eurecom.fr>",
    "Pasquale Lisena <pasquale.lisena@eurecom.fr>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DOREMUS-ANR/overture/issues"
  },
  "homepage": "https://github.com/DOREMUS-ANR/overture",
  "engines": {
    "node": "stable"
  }
}
