{
  "name": "overture",
  "version": "0.7.4",
  "description": "Exploring music data in DOREMUS",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"nodemon index\" \"ng serve --progress=false --proxy-config proxy.conf.json\"",
    "prebuild": "./node_modules/.bin/stylus ./src/styles/loading.styl -o ./src/styles/loading.css -u rupture -u nib",
    "build": "npm run prebuild && ng build --prod --output-hashing=none && ng run --output-hashing=none overture:server",
    "prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index"
  },
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/platform-server": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "async": "^2.6.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bestlang": "0.0.1",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.6",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "express": "^4.16.3",
    "express-content-length-validator": "1.0.0",
    "fs-extra": "^6.0.1",
    "get-json22": "^1.0.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.12.1",
    "jsonfile": "^4.0.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "ngx-infinite-scroll": "^0.8.4",
    "node-cache": "^4.2.0",
    "node-monkey": "^1.0.1",
    "node-noop": "^1.0.0",
    "reflect-metadata": "0.1.12",
    "require-dir": "^1.0.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
