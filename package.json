    "sparql-transformer": "^1.2.0",
    "ts-loader": "^4.3.0",
    "tsv": "^0.2.0",
    "typescript": "^2.7.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "^6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@types/core-js": "^0.9.46",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.1.2",
    "concurrently": "^3.5.1",
    "lodash": "^4.17.10",
    "material-shadows": "^3.0.1",
    "mdi": "^2.2.43",
