{
  "name": "overture",
  "version": "0.9.0",
  "description": "Exploring music data in DOREMUS",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"nodemon index\" \"ng serve --progress=false --proxy-config proxy.conf.json\"",
    "prebuild": "./node_modules/.bin/stylus ./src/styles/loading.styl -o ./src/styles/loading.css -u rupture -u nib",
    "build": "npm run prebuild && ng build --prod --output-hashing=none && ng run overture:server && npm run postbuild",
    "postbuild":"cpx \"src/styles/loading.css\" dist/styles/",
    "prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index"
  },
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/platform-server": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "accept-language-parser": "^1.5.0",
    "async": "^2.6.1",
    "bestlang": "0.0.1",
    "body-parser": "^1.18.3",
    "clone": "^2.1.2",
    "core-js": "^2.5.7",
    "es6-promise": "^4.2.5",
    "es6-shim": "^0.35.4",
    "express": "^4.16.4",
    "express-content-length-validator": "1.0.0",
    "fs-extra": "^7.0.1",
    "get-json22": "^1.0.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.15.0",
    "jsonfile": "^5.0.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "ng2-dnd": "^5.0.2",
    "ngx-infinite-scroll": "^6.0.1",
    "node-cache": "^4.2.0",
    "node-monkey": "^1.0.1",
    "node-noop": "^1.0.0",
    "reflect-metadata": "0.1.12",
    "require-dir": "^1.1.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "similarity": "^1.1.1",
    "sparql-client2": "^0.2.0",
    "sparql-transformer": "^1.6.1",
    "ts-loader": "^5.3.0",
    "tsv": "^0.2.0",
    "typescript": "^3.1.6",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.6",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "^7.0.4",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "^2.8.11",
    "@types/node": "^10.12.8",
    "concurrently": "^4.0.1",
    "cpx": "^1.5.0",
